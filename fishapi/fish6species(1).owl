  <fish:FishSpecies rdf:about="http://www.freshwaterfish.org/ontology#Puntius_titteya">
    <fish:Fish rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Puntius titteya</fish:Fish>
    <fish:ScientificName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Puntius titteya</fish:ScientificName>
    <fish:Authority rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Puntius titteya (Deraniyagala, 1929)</fish:Authority>
    <fish:SinhalaName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">le titteya</fish:SinhalaName>
    <fish:CommonName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cherry barb</fish:CommonName>
    <fish:ColourMorphsVariants rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detailed descriptions of the "neon" and "ruby" color varieties. Sexual color differences</fish:ColourMorphsVariants>
    <fish:EcologyBehavior rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Habitat preferences related to vegetation and water flow</fish:EcologyBehavior>
    <fish:MorphologyDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cherry Barb has an elongated body with a pair of maxillary barbels. Information about color variations ("neon" and "ruby" variants) Description of coloration patterns (horizontal line, fin colors) Sexual dimorphism details (males vs. females)</fish:MorphologyDescription>
    <fish:Order rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cypriniformes</fish:Order>
    <fish:Family rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cyprinidae</fish:Family>
    <fish:Subfamily rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Smiliogastrinae</fish:Subfamily>
    <fish:Habitat rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Recorded from shaded, slow flowing shallow streams with a muddy and leaf litter substratum. "Prefers areas with submerged aquatic vegetation</fish:Habitat>
    <fish:PHRange rdf:datatype="http://www.w3.org/2001/XMLSchema#string">6.0 - 8.0</fish:PHRange>
    <fish:DHRange rdf:datatype="http://www.w3.org/2001/XMLSchema#string">5.0-19</fish:DHRange>
    <fish:TemperatureRange rdf:datatype="http://www.w3.org/2001/XMLSchema#string">23°C - 27°C</fish:TemperatureRange>
    <fish:Latitude rdf:datatype="http://www.w3.org/2001/XMLSchema#string">8°N - 6°N</fish:Latitude>
    <fish:Region rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Asia</fish:Region>
    <fish:Countries rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sri Lanka</fish:Countries>
    <fish:SpecificLocation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kelani to Nilwala basins</fish:SpecificLocation>
    <fish:StatusNotes rdf:datatype="http://www.w3.org/2001/XMLSchema#string">More colorful varieties are possibly being overfished for the aquarium trade</fish:StatusNotes>
    <fish:MaximumLengthCm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">5.0 cm TL (male/unsexed</fish:MaximumLengthCm>
    <fish:CommonLengthCm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2.5 cm TL</fish:CommonLengthCm>
    <fish:Maturity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unknown</fish:Maturity>
    <fish:Distribution rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference to Nilwala and Gin basins (for the "Ruby" colored form)</fish:Distribution>
    <fish:HabitatDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Recorded from shaded, slow flowing shallow streams with a muddy and leaf litter substratum." "Prefers areas with submerged aquatic vegetation.</fish:HabitatDescription>
    <fish:Diet rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Omnivorous (detritus, green algae, diatoms, dipterans, and animal matter)</fish:Diet>
    <fish:Reproduction rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eggs: ~200 eggs scattered among marginal plants</fish:Reproduction>
    <fish:FreeSwimmingAfter rdf:datatype="http://www.w3.org/2001/XMLSchema#string">48 hours</fish:FreeSwimmingAfter>
    <fish:Hatching rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1-2 days</fish:Hatching>
    <fish:AquariumGroupSize rdf:datatype="http://www.w3.org/2001/XMLSchema#string">5 or more</fish:AquariumGroupSize>
    <fish:AquariumMinimumTankSizeCm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">60 cm</fish:AquariumMinimumTankSizeCm>
    <fish:IUCNStatus rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vulnerable (VU)</fish:IUCNStatus>
    <fish:ThreatToHumans rdf:datatype="http://www.w3.org/2001/XMLSchema#string">harmless</fish:ThreatToHumans>
    <fish:FisheriesValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">highly commercial in aquarium trade</fish:FisheriesValue>
  </fish:FishSpecies>
  <fish:FishSpecies rdf:about="http://www.freshwaterfish.org/ontology#Devario_pathirana">
    <fish:Fish rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Devario pathirana</fish:Fish>
    <fish:ScientificName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Devario pathirana</fish:ScientificName>
    <fish:Authority rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kottelat &amp; Pethiyagoda, 1990</fish:Authority>
    <fish:SinhalaName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pathirana saalaya</fish:SinhalaName>
    <fish:CommonName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Barred Danio,Sri Lanka Barred Danio</fish:CommonName>
    <fish:ColourMorphsVariants rdf:datatype="http://www.w3.org/2001/XMLSchema#string">No known variants; consistent metallic blue bars on a golden background.</fish:ColourMorphsVariants>
    <fish:EcologyBehavior rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Non-migratory, forms small groups (3-5 individuals), moves near the surface, more sedentary than Devario malabaricus.</fish:EcologyBehavior>
    <fish:MorphologyDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compressed body, dorsally greenish-brown, lighter laterally with metallic blue bars on a golden background, silvery-white ventral side. Transparent fins with a yellow tint; caudal fin with dark rays forming a horizontal black band.</fish:MorphologyDescription>
    <fish:Order rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cypriniformes</fish:Order>
    <fish:Family rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Danionidae</fish:Family>
    <fish:Subfamily rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Danioninae</fish:Subfamily>
    <fish:Etymology rdf:datatype="http://www.w3.org/2001/XMLSchema#string">"Devario" originates from the Bengali word ‘debari.’ Named after Ananda ‘Andy’ Pathirana, a Sri Lankan aquarium-fish breeder and exporter.</fish:Etymology>
    <fish:Habitat rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Freshwater, prefers still pools over fast-flowing areas.</fish:Habitat>
    <fish:TemperatureRange rdf:datatype="http://www.w3.org/2001/XMLSchema#string">22°C - 25°C</fish:TemperatureRange>
    <fish:Latitude rdf:datatype="http://www.w3.org/2001/XMLSchema#string">7°N - 6°N</fish:Latitude>
    <fish:Region rdf:datatype="http://www.w3.org/2001/XMLSchema#string">South Asia</fish:Region>
    <fish:Countries rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sri Lanka</fish:Countries>
    <fish:SpecificLocation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nilwala River basin, including Opatha, Mawarala, Dediyagala, Pitabeddara, and Kamburupitiya.</fish:SpecificLocation>
    <fish:StatusNotes rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Endemic species with a limited distribution, threatened due to habitat degradation.</fish:StatusNotes>
    <fish:MaximumLengthCm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">6.0 cm SL</fish:MaximumLengthCm>
    <fish:CommonLengthCm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">~5-6 cm SL</fish:CommonLengthCm>
    <fish:Distribution rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Endemic to Sri Lanka, previously thought to be restricted to Opatha but later found in other areas within the Nilwala River basin.</fish:Distribution>
    <fish:HabitatDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Found in clear, shaded streams with rocky or pebble substrate, prefers still pools rather than fast currents. Avoids silted rivulets.</fish:HabitatDescription>
    <fish:Diet rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Likely feeds on insects.</fish:Diet>
    <fish:AquariumGroupSize rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Prefers small groups (3-5 individuals).</fish:AquariumGroupSize>
    <fish:IUCNStatus rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Endangered (EN) – IUCN Red List (2019)</fish:IUCNStatus>
    <fish:ThreatToHumans rdf:datatype="http://www.w3.org/2001/XMLSchema#string">harmless</fish:ThreatToHumans>
    <fish:FisheriesValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Occasionally exported in the aquarium trade, but collection is limited due to conservation concerns.</fish:FisheriesValue>
  </fish:FishSpecies>
